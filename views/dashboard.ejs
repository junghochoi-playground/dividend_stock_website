<!-- 

    Variables open to this EJS Template

    securities - [] array of securities



 -->



<%- include("partials/header.ejs") %>
<%- include("partials/searchbar.ejs") %> 



<div class="container">

<h1>Welcome <%= currentUser.username %>! </h1>


<form action="/dashboard/search" method="GET">
    <input type="text" name="ticker" placeholder="ticker">
    <input type="submit">
</form>




<table>
<% portfolio.forEach(ticker =>{ %>
    <tr>
        <td>
            <a href="/dashboard/<%= ticker %> "><%= ticker %> </a>
        </td>
        <td>
            <a href="/dashboard/<%= ticker %>/news"><%= ticker %> News</a>
        </td>
    </tr>
<% }); %>  
</table>
</div>

 




<%- include("partials/footer.ejs") %>